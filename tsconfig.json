{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "baseUrl": "./src",
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react",
    "typeRoots": [
      "./types"
    ]
  }
}
